{
  "name": "jmc",
  "displayName": "JMC",
  "description": "Basic syntax highlighting for JMC(JavaScript-like Minecraft Function)",
  "repository": {"url": "https://github.com/WingedSeal/JMC-Syntax-Highlighting"},
  "version": "0.0.1",
  "publisher": "WingedSeal",
  "engines": {
    "vscode": "^1.72.0"
  },
  "icon": "images/jmc_icon.png",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "jmc",
      "aliases": ["JMC", "jmc"],
      "extensions": [".jmc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "jmc",
      "scopeName": "source.jmc",
      "path": "./syntaxes/jmc.tmLanguage.json"
    }]
  }
}
